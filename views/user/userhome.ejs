          fetch('/wishlist/add', {
            method: 'POST',
            headers: {
              'Content-Type': 'application/json',
            },
            body: JSON.stringify({
              productId: productId ,
            }),
          })
          .then(response => response.json())
          .then(data => {
            console.log(data);
            // Optionally, you can provide feedback to the user here
          })
          .catch(error => {
