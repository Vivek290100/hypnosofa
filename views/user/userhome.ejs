<% let count = 0; %>
<% for (let index = 0; index < products.length; index++) { %>
    <% if (count >= 8) { break; } %>
    <% count++; %>
    <% const product = products[index]; %>
    <% if (index % 4 === 0) { %>
        <div class="row">
    <% } %>
    <div class="container">
        <% if (product.images && product.images.length > 0) { %>
            <img src="/assets/product-images/<%= product.images[0] %>" class="banner-image" id="image-zoom" alt="<%= product.name %>"
                onclick="openModal('<%= product.images[0] %>')"><br>
        <% } else { %>
            <img src="/assets/default-image.jpg" class="banner-image" id="image-zoom" alt="Default Image"><br>
        <% } %>
        <h5 class="card-title"><%= product.name %></h5><br>
        <!-- <h3><%= "₹" + product.price %></h3> -->
        <div class="button-wrapper d-flex">
            <a href="/user/mainproduct/<%= product._id %>"><button class="btn outline">VIVEW DETAILS</button></a>
            <!-- <div class="wishlist"><i class="fa-solid fa-heart"></i></div> -->
            <button class="wishlist btn wishlist-btn outline" data-product-id="<%= product._id %>" >
                <i class="fa-solid fa-heart"></i>
            </button>
        </div>
    </div>
    <% if ((index + 1) % 4 === 0 || (index + 1) === products.length || count === 8) { %>
        </div>
    <% } %>
<% } %>

